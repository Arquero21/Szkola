body {
	background-color: rgb(129, 58, 180);
}
input {
	background-color: rgb(251, 255, 0);
	font-family: "Gill Sans", "Gill Sans MT", Calibri, "Trebuchet MS", sans-serif;
	margin: 250px 500px;
	width: 500px;
	height: 80px;
	display: block;
	font-size: 1em;
	font-style: italic;
	font-weight: bold;
	color: rgb(0, 0, 0);
	border: 5px solid rgb(42, 44, 184);
	border-radius: 20px;
}
